{
  "name": "QuizTab",
  "version": "1.0",
  "description": "Utilizes the Quizlet API to help you study every time a new tab opens!",
  "permissions": [
    "identity",
    "storage",
    "https://www.quizlet.com/*",
    "https://api.quizlet.com/*",
    "https://fonts.googleapis.com/*"
  ],
  "chrome_url_overrides": {
		"newtab": "index.html"
	},
  "web_accessible_resources": [
    "assets/css/style.css",
    "assets/fonts/Raleway-Bold.ttf",
    "assets/*"
  ],
  "manifest_version": 2
}
